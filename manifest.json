{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "0.1",
    "default_locale": "en",
    "background": {
        "scripts": ["background.js"]
    },
    "homepage_url": "https://github.com/jbellue/intensify-webext",
    "permissions": [
        "storage",
        "contextMenus",
        "activeTab",
        "tabs",
        "<all_urls>"
    ],
    "options_ui": {
        "page": "options/options.html",
        "browser_style": true
    }
}
